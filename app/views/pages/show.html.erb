<div class="page">
  <h2 id="name">
    Phil Zheng
  </h2>

<div class=box-shadow>
  <div class="page__header">
    Search Term
  </div>
</div>

  <div class="tabs">
    <div class="tab-menu">
      <div class="tab-menu__item selected" target="#page1">Page 1</div>
      <div class="tab-menu__item" target="#page2">Page 2</div>
    </div>
  
    <div class="tab-content " id="page1">
      <div class="card box-shadow">
      <% @videos[:items].each do |videos| %>
        <%= videos[:snippet][:title] %>
      <% end %>
      </div>
    </div>
  
    <div class="tab-content hidden" id="page2">
      <div class="card box-shadow">
        page2
      </div>
    </div>
  </div>

  <footer>

  </footer>

</div>
